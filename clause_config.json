{
  "clause_patterns": {
    "numbered": "(\\d{1,2})\\.\\s*([A-Z][A-Za-z\\s]*?)[:.]?\\s*(.*?)(?=\\n\\s*\\d{1,2}\\.\\s*[A-Z]|$)",
    "lettered": "\\(([a-z])\\)\\s*([A-Z][A-Za-z\\s]*?)[:.]?\\s*(.*?)(?=\\n\\s*\\([a-z]\\)|$)",
    "section_headers": "([A-Z][A-Z\\s]{3,}):?\\s*(.*?)(?=[A-Z][A-Z\\s]{3,}:|$)",
    "paragraph_fallback": "([A-Z][A-Za-z\\s]*?)[:.]?\\s*(.*?)(?=\\n\\n|$)"
  },
  "document_specific_patterns": {
    "employment": {
      "clause_definitions": [
        [
