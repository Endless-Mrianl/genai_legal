          "Compensation and Benefits",
          "As full compensation for all services"
        ],
        [
          "4",
          "Miscellaneous Expenses",
          "Business and Travel Expenses"
        ],
        [
          "5",
          "Termination",
          "Termination by the Board"
        ],
        [
          "6",
          "Entire Agreement",
          "This Agreement constitutes the entire understanding"
        ],
        [
          "7",
          "Waiver",
          "No delay or failure to enforce"
        ],
        [
          "8",
          "Governing Law",
          "This Agreement shall be construed and enforced"
        ],
        [
          "9",
          "No Assignment",
          "This Agreement and all of the Employee's rights"
        ],
        [
          "10",
          "Binding Effect",
          "This Agreement shall inure to the benefit"
        ],
        [
          "11",
          "Notices",
          "All notices or other communications"
        ],
        [
          "12",
          "Counterparts",
          "This Agreement may be executed"
        ],
        [
          "13",
          "Severability",
          "In the event any one or more"
        ],
        [
          "14",
          "Recitals",
          "The above Recitals are true"
        ]
      ]
    },
    "rental": {
      "clause_patterns": [
        "(\\d+)\\.\\s*DURATION[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*RENT[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*DEPOSIT[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*ELECTRICAL[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*MAINTENANCE[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*LIABLE[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*USER[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
        "(\\d+)\\.\\s*TAXES[:\\s]*(.*?)(?=\\d+\\.\\s*[A-Z]|$)",
